class CharacterCount
{
int count=0;
String text;
public int charcount(String text)
{
	int i;
	for(i=0;i<text.length();i++)
	{
		if(text.charAt(i)!=' ')
		{
			count=count+1;
		}
	}
	return count;
	//System.out.println("Total no of character is"+count);
}
}
