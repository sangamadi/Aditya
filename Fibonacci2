class Fib
{
	int n,m,k;
	public void fibonacci(int t)
	{n=2;
	m=3;
	System.out.print(n+" "+m+" ");
	for(int i=0;i<=t;i++)
	{
		k=n+m;
		n=m;
		m=k;
		System.out.print(k+" ");	
	}
		
	}
	}
public class FibonacciExp2 {
	public static void main(String argsp[])
	{
		int t=12;
		Fib a= new Fib();
		a.fibonacci(t);
	}

}
